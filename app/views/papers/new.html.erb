
			<h3> 新規投稿</h3>
			<div class="">
				<% if @paper.errors.any? %>
				<div id="error_explanation" class="alert alert-danger">
					<h2  class="alert_h2"><%= pluralize(@paper.errors.count, "error") %> prohibited this artist from being saved:</h2>
					<ul>
						<% @paper.errors.full_messages.each do |message| %>
						<li><%= message %></li>
						<% end %>
					</ul>
				</div>
				<% end %>
				<%= form_for(@paper) do |f| %>
					<h4> タイトル</h4>
					<%= f.text_field :title, placeholder:"タイトル", class:"data" %>
					<h4> 内容</h4>
					<%= f.text_area :content, placeholder:"内容", class:"data" %>
					<%= f.select :genre_id,Genre.all.collect { |p| [ p.name, p.id ] }, {include_blank: true}, {class: 'data'} %>
					<%= f.date_field :start_date %>から
					<%= f.date_field :end_date %>まで

					<%= f.fields_for :paper_images do |builder| %>
					<%= link_to_add_fields "画像追加", f, :paper_images %>
						<%= f.submit class: "btn btn-primary data" %>
					<% end %>
				<% end %>
			</div>